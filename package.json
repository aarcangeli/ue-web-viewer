{
  "name": "ue-web-viewer",
  "version": "0.1.0",
  "type": "module",
  "author": "Alessandro Arcangeli <alessandroarcangeli.rm@gmail.com>",
  "repository": "https://github.com/aarcangeli/ue-web-viewer",
  "homepage": "https://aarcangeli.github.io/ue-web-viewer",
  "license": "MIT",
  "scripts": {
    "postinstall": "tsx scripts/generate_externals.ts",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage --reporter=junit --outputFile=junit.xml",
    "test:update-snapshots": "vitest run -u",
    "generate:icons": "ts-node scripts/generate-icons.ts ./src/UeWebViewer/icons ./src/UeWebViewer/icons.gen.ts",
    "typecheck": "tsc --noEmit",
    "eslint": "eslint . --fix",
    "eslint:dump-config": "node eslint.config.js --dump",
    "eslint:inspector": "eslint-config-inspector",
    "export-layout": "tsx scripts/ExportLayout/export-layout.ts",
    "pre-commit": "yarn typecheck && yarn eslint && vitest run && pre-commit run -a"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.1.1",
    "@chakra-ui/react": "^2.8.2",
    "@codecov/vite-plugin": "^1.9.1",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@eslint/config-inspector": "^0.5.4",
    "@eslint/js": "^9.12.0",
    "@swc/core": "^1.7.35",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/wicg-file-system-access": "^2023.10.5",
    "@vitejs/plugin-react-swc": "^3.7.1",
    "@vitest/coverage-v8": "^4.0.16",
    "eslint": "^8.57.1",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.12",
    "framer-motion": "^11.9.0",
    "globals": "^15.10.0",
    "prettier": "^3.2.5",
    "react": "^18.3.1",
    "react-arborist": "^3.4.0",
    "react-dom": "^18.3.1",
    "react-icons": "^5.2.0",
    "tiny-invariant": "^1.3.3",
    "ts-morph": "^26.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.5.0",
    "typescript-eslint": "^8.8.0",
    "use-resize-observer": "^9.1.0",
    "vite": "^7.3.0",
    "vite-plugin-json5": "^1.1.2",
    "vite-plugin-svgr": "^4.5.0",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^4.0.16",
    "web-vitals": "^2.1.0"
  }
}
